{
  "kind": "collectionType",
  "collectionName": "achievements",
  "info": {
    "name": "Achievement"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "description": {
      "type": "richtext"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "name": {
      "type": "string"
    },
    "users": {
      "via": "achievements",
      "plugin": "users-permissions",
      "collection": "user"
    }
  }
}
