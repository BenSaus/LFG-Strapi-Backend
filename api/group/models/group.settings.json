{
  "kind": "collectionType",
  "collectionName": "groups",
  "info": {
    "name": "Group"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "description": {
      "type": "richtext"
    },
    "open_slots": {
      "type": "integer"
    },
    "booking_status": {
      "type": "enumeration",
      "enum": [
        "booked",
        "notBooked"
      ]
    },
    "applications": {
      "via": "group",
      "collection": "application"
    },
    "members": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "groups",
      "dominant": true
    },
    "invites": {
      "via": "group",
      "collection": "invite"
    },
    "max_age": {
      "type": "integer"
    },
    "min_age": {
      "type": "integer"
    },
    "bookdate": {
      "type": "datetime"
    },
    "name": {
      "type": "string",
      "unique": false,
      "required": false
    },
    "leader": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "leading_groups"
    },
    "preferred_rooms": {
      "collection": "room",
      "via": "groups_preferred",
      "dominant": true
    }
  }
}
