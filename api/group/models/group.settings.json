{
  "kind": "collectionType",
  "collectionName": "groups",
  "info": {
    "name": "Group",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "description": {
      "type": "richtext"
    },
    "member_max": {
      "type": "integer",
      "required": false
    },
    "booking_status": {
      "type": "enumeration",
      "enum": [
        "booked",
        "notBooked"
      ]
    },
    "applications": {
      "collection": "application",
      "via": "group"
    },
    "members": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "groups",
      "dominant": true
    },
    "invites": {
      "collection": "invite",
      "via": "group"
    },
    "max_age": {
      "type": "integer"
    },
    "min_age": {
      "type": "integer"
    },
    "bookdate": {
      "type": "datetime"
    },
    "name": {
      "type": "string",
      "unique": false,
      "required": false
    },
    "leader": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "leading_groups"
    },
    "preferred_rooms": {
      "collection": "room",
      "via": "groups_preferred",
      "dominant": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "open",
        "closed",
        "booking",
        "booked"
      ],
      "default": "open"
    },
    "preferred_date_times": {
      "collection": "preferred-date-time",
      "via": "group"
    }
  }
}
